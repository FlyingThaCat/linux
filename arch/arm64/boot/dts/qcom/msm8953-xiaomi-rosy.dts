/dts-v1/;

#include "msm8953-xiaomi-common.dtsi"

/delete-node/ &adsp_fw_mem;
/delete-node/ &qseecom_mem;
/delete-node/ &wcnss_fw_mem;

/* Decrease the size */
&cont_splash_mem {
	reg = <0x0 0x90001000 0x0 (720 * 1440 * 3)>;
};

/ {
	model = "Xiaomi Redmi 5";
	compatible = "xiaomi,rosy", "qcom,msm8953";
	qcom,board-id = <0x1000b 1>;

	chosen {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		framebuffer@90001000 {
			compatible = "simple-framebuffer";
			reg = <0 0x90001000 0 (720 * 1440 * 3)>;

			width = <720>;
			height = <1440>;
			stride = <(720 * 3)>;
			format = "r8g8b8";

			power-domains = <&gcc MDSS_GDSC>;

			clocks = <&gcc GCC_MDSS_AHB_CLK>,
				 <&gcc GCC_MDSS_AXI_CLK>,
				 <&gcc GCC_MDSS_VSYNC_CLK>,
				 <&gcc GCC_MDSS_MDP_CLK>,
				 <&gcc GCC_MDSS_BYTE0_CLK>,
				 <&gcc GCC_MDSS_PCLK0_CLK>,
				 <&gcc GCC_MDSS_ESC0_CLK>;
		};
	};

    reserved-memory {
        adsp_fw_mem: adsp@8d600000 {
            reg = <0x0 0x8d600000 0x0 0x1200000>;
            no-map;
        };

        qseecom_mem: qseecom@84a00000 {
			reg = <0x0 0x84a00000 0x0 0x1e00000>;
			no-map;
		};

        wcnss_fw_mem: wcnss@8e700000 {
            reg = <0x0 0x8e700000 0x0 0x700000>;
            no-map;
        };

		ramoops@9ff00000 {
			compatible = "ramoops";
			reg = <0x0 0x9ff00000 0x0 0x00100000>;
			record-size = <0x1000>;
			console-size = <0x80000>;
			ftrace-size = <0x1000>;
			pmsg-size = <0x8000>;
		};
    };
};

&panel {
	compatible = "xiaomi,rosy-panel";
};

&pmi8950_wled {
	qcom,external-pfet;
	qcom,cabc;
};

&battery {
	charge-full-design-microamp-hours = <4000000>;
	constant-charge-current-max-microamp = <1000000>;
	voltage-min-design-microvolt = <3400000>;
	voltage-max-design-microvolt = <4400000>;
};

&tlmm {
	gpio-reserved-ranges = <0 4>, <16 4>, <135 4>;
};

&uart_0 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart_console_active>;

	status = "okay";
};